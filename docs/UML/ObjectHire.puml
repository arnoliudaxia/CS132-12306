@startuml
'https://plantuml.com/class-diagram

class TrainDispatch{
datetime SysTime
List<Station> Stations
List<Train> Trains
}

class Train{
 string trainCode
 trainStatus status
 List<Station> remainingStations
 int lineType
 bool lineDirection
' List<Passanger> passangers
}

class Passanger{
int passangerId
string passangerName
Train bookTrain
Passangertatus myStatus
List<Ticket> myTickets

bool bookTicket(Ticket)

}

class Station{
    string stationName
    datetime arrivalTime
    datetime departureTime
}

TrainDispatch *-- Train
TrainDispatch *-- Passanger
TrainDispatch *-- Station

class Ticket{
    Train ticketTrain
'    string ticketType
'    int ticketPrice
    bool isOcupied
    Passanger bookPassanger
    Station fromStation
    Station toStation
}
Station *.. Train
Ticket *-- Station
Ticket *.. Passanger

TrainDispatch *-- Ticket

enum trainStatus{
    '等待发车
    NOTSTARTED
    'train is running
    RUNNING
    'train is stopped
    STOPPED
}

Train *-- trainStatus

@enduml
